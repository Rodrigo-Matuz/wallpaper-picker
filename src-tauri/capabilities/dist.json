{
    "identifier": "dist-capability",
    "description": "Capabilities for distribution",
    "windows": ["*"],
    "permissions": [
        "core:event:allow-emit",
        "core:path:allow-basename",
        "core:resources:allow-close",
        "core:window:allow-close",
        "core:path:default",
        {
            "identifier": "fs:allow-read-dir",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-read",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-write",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-app-read",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-app-read-recursive",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-open",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:scope-appconfig",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:scope-appconfig-index",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:scope-appconfig-recursive",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-meta",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-meta-recursive",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-read",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-read-recursive",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-write",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-appconfig-write-recursive",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:read-all",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        },
        {
            "identifier": "fs:allow-exists",
            "allow": [{ "path": "$APPDATA/**" }, { "path": "$APPCONFIG/**" }]
        }
    ]
}
