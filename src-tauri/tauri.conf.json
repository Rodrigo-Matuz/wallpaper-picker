{
	"productName": "wallpaper_picker",
	"version": "1.0.0",
	"identifier": "wallpaper_picker",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "wallpaper_picker",
				"width": 950,
				"height": 600,
				"fullscreen": false
			}
		],
		"security": { "capabilities": ["dist-capability"] }
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
